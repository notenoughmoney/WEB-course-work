1 level
Naruto.moveDown()
Naruto.moveRight(5);

2 level
Naruto.moveRight(2)
Naruto.moveDown(2)
Naruto.moveRight(7)

3 level
Naruto.moveDown()
Sakura.moveLeft()
Sasuke.moveUp()
Naruto.moveRight(3)

4 level
Naruto.moveRight(2)
Naruto.hitRight(4)
Naruto.moveRight(4)

5 level
//Берсерк
Sasuke.moveRight(3)
Sasuke.hitRight()
Sasuke.moveDown()
Sasuke.hitRight()
Sasuke.moveDown()
Sasuke.hitRight()
Naruto.moveRight(6)
// Команда
Sasuke.moveRight(3)
Sasuke.hitRight()
Naruto.moveRight(2)
Naruto.hitRight()
Sakura.moveRight(3)
Sakura.hitRight()
Naruto.moveRight(7)